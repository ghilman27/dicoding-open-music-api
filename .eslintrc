{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": [1, { "allowAfterThis": true }],
        "object-curly-newline": "off"
    },
    "parser": "babel-eslint"
}
